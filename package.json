    "vite-plugin-pwa": "^1.0.0",
    "vitest": "^1.3.1"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,md}": [
      "prettier --write"
    ]
  }
