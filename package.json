    "react-loading-skeleton": "^3.5.0",
    "react-masonry-css": "^1.0.16",
    "react-photoswipe-gallery": "^3.1.1",
    "react-router-dom": "^7.6.0",
    "react-slick": "^0.30.3",
    "react-toastify": "^11.0.5",
    "react-type-animation": "^3.2.0",
    "sharp": "^0.34.2",
    "slick-carousel": "^1.8.1",
    "styled-components": "^6.1.18",
    "swiper": "^11.2.6",
