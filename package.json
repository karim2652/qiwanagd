{
  "name": "qiwanagd",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "node copy-assets.js && vite build",
    "lint": "eslint . --ext .js,.jsx --fix",
    "preview": "vite preview",
    "format": "prettier --write \"src/**/*.{js,jsx,css,md}\"",
    "analyze": "vite build --mode analyze",
    "test": "vitest",
    "prepare": "husky install",
    "copy-assets": "node copy-assets.js",
    "build:prod": "NODE_ENV=production vite build",
    "preview:prod": "vite preview --mode production",
    "lighthouse:desktop": "lhci autorun --config=lighthouserc.cjs",
    "lighthouse:mobile": "lhci autorun --config=lighthouserc.mobile.cjs",
    "lighthouse": "npm run lighthouse:desktop && npm run lighthouse:mobile"
  },
  "dependencies": {
